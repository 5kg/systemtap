/* Make sure however we entered a syscall, we know the calling module name. The
   The task finder vma tracker should have recorded it. */
probe syscall.clock_gettime, syscall.getuid
{
   if (target() == pid())
   {
      try
      {
         umodname(uaddr())
         log(name)
      }
      catch
      {
         printf("%s@%x unknown\n", name, uaddr());
      }
   }
}
