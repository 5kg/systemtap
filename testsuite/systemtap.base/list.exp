# PR14297-related testing

set test plt-glob
spawn stap -l {process("stap").plt("re??")}
expect {
    -re {process."stap"..plt."read".\r\n} { pass $test }
    -re {^[^\r\n]*\r\n} { exp_continue }
    timeout { fail $test }
    eof { fail $test }
}
catch { close }; catch { wait }

